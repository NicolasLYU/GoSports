    <mkdir dir="${login-impl.output.dir}"/>
    <javac2 destdir="${login-impl.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.login-impl}/javac">
      <compilerarg line="${compiler.args.login-impl}"/>
      <bootclasspath refid="login-impl.module.bootclasspath"/>
      <classpath refid="login-impl.module.production.classpath"/>
      <src refid="login-impl.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.login-impl"/>
    </javac2>
    
    <copy todir="${login-impl.output.dir}">
      <fileset dir="${module.login-impl.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
